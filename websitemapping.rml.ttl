@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#lat>.
@prefix gn: <http://www.geonames.org/ontology#>.
@prefix ex: <http://www.example.taxonic.com#>.
@prefix up: <http://purl.oclc.org/NET/UNIS/up#>.
@prefix carml: <http://carml.taxonic.com/carml/> .

<#MedewerkerMapping>
  rml:logicalSource [
    rml:source "medewerkerOutputDictwithURL.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.medewerker";
  ];

  rr:subjectMap [
    rr:template "http://example.com/person/{name}";
    rr:class schema:Person 
  ];

  rr:predicateObjectMap [
    rr:predicate schema:name;
    rr:objectMap [
      rml:reference "name" 
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate schema:jobTitle;
    rr:objectMap [
      rml:reference "title" 
    ]
  ];

   rr:predicateObjectMap [
    rr:predicate foaf:depiction;
    rr:objectMap [
      rml:reference "depiction";
	  rr:termType	rr:IRI
    ]
  ];
  
     rr:predicateObjectMap [
    rr:predicate schema:url;
    rr:objectMap [
      rml:reference "url";
	  rr:termType	rr:IRI
    ]
  ];
  
    rr:predicateObjectMap [
    rr:predicate foaf:worksWith ;
    rr:objectMap [
      carml:multiReference "clients" ;
      rr:datatype xsd:string ;
    ] 
  ] ;
  
      rr:predicateObjectMap [
    rr:predicate up:Ability ;
    rr:objectMap [
      carml:multiReference "talents" ;
      rr:datatype xsd:string ;
    ] ;
  ] .
  
  <#ImageMapping>
  rml:logicalSource [
    rml:source "medewerkerOutputDictwithURL.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.medewerker";
  ];

  rr:subjectMap [
    rml:reference "depiction";
    rr:class foaf:Image;
  ] .
  
    <#URLMapping>
  rml:logicalSource [
    rml:source "medewerkerOutputDictwithURL.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$.medewerker";
  ];

  rr:subjectMap [
    rml:reference "url";
    rr:class schema:URL ;
  ] .